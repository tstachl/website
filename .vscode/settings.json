{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
  "grammarly.domain": "casual",
  "grammarly.showExamples": true,
  "grammarly.autoActivate": true,
  "grammarly.showUsernameInStatusBar": true,
  "editor.wordWrap": "wordWrapColumn",
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": false
  },
  "editor.wordWrapColumn": 80,
  "grammarly.overrides": [
    {
      "files": [
        "**/src/pages/posts/token-valuation.md"
      ],
      "config": {
        "audience": "knowledgeable",
        "dialect": "american",
        "domain": "casual",
        "emotions": [
          "confident",
          "optimistic",
          "analytical"
        ],
        "goals": [
          "inform"
        ]
      }
    }
  ]
}